邮箱服务器
docker-compose.yml:

environment:
    SENTRY_EMAIL_HOST: 'smtp.qq.com'
    SENTRY_EMAIL_PORT: '587'
    SENTRY_EMAIL_USER: ''
    SENTRY_SERVER_EMAIL: ''
    SENTRY_EMAIL_PASSWORD: ''
    SENTRY_EMAIL_USE_TLS: 'True'
